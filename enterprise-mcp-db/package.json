{
  "name": "enterprise-mcp-smart-database",
  "version": "1.0.0",
  "description": "Revolutionary Multi-MCP Smart Database System - Next-generation database replacement technology",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run build:webpack",
    "build:webpack": "webpack --mode production",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:performance": "vitest run tests/performance",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src tests docs",
    "security:audit": "npm audit && snyk test",
    "docker:build": "docker build -t enterprise-mcp-db .",
    "docker:run": "docker run -p 8080:8080 enterprise-mcp-db",
    "docs:generate": "typedoc src --out docs/api",
    "benchmark": "node scripts/benchmark.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js"
  },
  "keywords": [
    "database",
    "mcp",
    "enterprise",
    "smart-database",
    "multi-protocol",
    "distributed",
    "real-time",
    "high-performance"
  ],
  "author": "Enterprise MCP Development Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/mcp": "^1.0.0",
    "fastify": "^4.25.2",
    "fastify-plugin": "^4.5.1",
    "fastify-cors": "^9.0.1",
    "fastify-helmet": "^11.1.1",
    "fastify-rate-limit": "^9.1.0",
    "ws": "^8.16.0",
    "ioredis": "^5.3.2",
    "sqlite3": "^5.1.6",
    "better-sqlite3": "^9.2.2",
    "pg": "^8.11.3",
    "mongodb": "^6.3.0",
    "prisma": "^5.8.1",
    "@prisma/client": "^5.8.1",
    "zod": "^3.22.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "crypto-js": "^4.2.0",
    "winston": "^3.11.0",
    "pino": "^8.17.2",
    "prometheus-client": "^1.1.0",
    "dotenv": "^16.3.1",
    "joi": "^17.12.0",
    "ajv": "^8.12.0",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "vitest": "^1.2.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "prettier": "^3.2.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "ts-loader": "^9.5.1",
    "webpack-node-externals": "^3.0.0",
    "typedoc": "^0.25.7",
    "snyk": "^1.1276.0",
    "supertest": "^6.3.4",
    "@types/supertest": "^6.0.2",
    "autocannon": "^7.14.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enterprise/mcp-smart-database.git"
  },
  "bugs": {
    "url": "https://github.com/enterprise/mcp-smart-database/issues"
  },
  "homepage": "https://github.com/enterprise/mcp-smart-database#readme"
}